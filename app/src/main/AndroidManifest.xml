<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.feevale.movery">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".principal"
            android:label="Movery">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".nova_encomenda"
            android:label="@string/st_novaEncomenda" />
        <activity
            android:name=".endereco"
            android:label="Endereço"></activity>
        <activity android:name=".configuracao"
            android:label="Configurações">
        </activity>
    </application>

</manifest>